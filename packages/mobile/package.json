{
  "name": "@xaui/mobile",
  "version": "0.0.2",
  "description": "Mobile UI components for XUI",
  "keywords": [
    "react-native",
    "mobile",
    "ui",
    "components",
    "xaui"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js"
    },
    "./progress": {
      "types": "./dist/progress/index.d.ts",
      "import": "./dist/progress/index.js",
      "require": "./dist/progress/index.js"
    },
    "./indicator": {
      "types": "./dist/indicator/index.d.ts",
      "import": "./dist/indicator/index.js",
      "require": "./dist/indicator/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rygrams/xaui.git",
    "directory": "packages/mobile"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@xaui/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native": ">=0.70.0",
    "react-native-reanimated": ">=4.0.0",
    "react-native-svg": ">=15.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-native": "^0.73.0",
    "react": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-reanimated": "^4.0.0",
    "react-native-svg": "^15.8.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
